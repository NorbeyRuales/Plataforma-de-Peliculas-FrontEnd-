{
    "plugins": [
        "plugins/markdown",
        "node_modules/jsdoc-babel"
    ],
    "source": {
        "include": [
            "./src"
        ],
        "includePattern": ".+\\.(ts|tsx|js|jsx)$",
        "excludePattern": "(^|\\/|\\\\)_|\\.d\\.ts$"
    },
    "babel": {
        "extensions": ["js", "jsx", "ts", "tsx"],
        "ignore": ["**/*.d.ts"],
        "configFile": "./babel.config.cjs"
    },
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "template": "node_modules/docdash"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}
